{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/products",
        "dest": "index.js"
      },
      {
        "src": "/products/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/products/payment/process",
        "dest": "index.js"
      },
      {
        "src": "/products/payment/getkey",
        "dest": "index.js"
      },
      {
        "src": "/products/payment/verification",
        "dest": "index.js"
      },
      {
        "src": "/categories",
        "dest": "index.js"
      },
      {
        "src": "/categories/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/brands",
        "dest": "index.js"
      },
      {
        "src": "/brands/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/users",
        "dest": "index.js"
      },
      {
        "src": "/users/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/auth",
        "dest": "index.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/cart",
        "dest": "index.js"
      },
      {
        "src": "/cart/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/orders",
        "dest": "index.js"
      },
      {
        "src": "/orders/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "dist/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "dist/$1"
      }
    ]
  }
  