{
  "functions": {
    "api/index.js": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/products",
      "dest": "api/index.js"
    },
    {
      "src": "/products/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/products/payment/process",
      "dest": "api/index.js"
    },
    {
      "src": "/products/payment/getkey",
      "dest": "api/index.js"
    },
    {
      "src": "/products/payment/verification",
      "dest": "api/index.js"
    },
    {
      "src": "/categories",
      "dest": "api/index.js"
    },
    {
      "src": "/categories/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/brands",
      "dest": "api/index.js"
    },
    {
      "src": "/brands/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/users",
      "dest": "api/index.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/auth",
      "dest": "api/index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/cart",
      "dest": "api/index.js"
    },
    {
      "src": "/cart/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/orders",
      "dest": "api/index.js"
    },
    {
      "src": "/orders/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/",
      "dest": "dist/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "dist/$1"
    }
  ]
}
